	<subDataset name="cotizacion_producto" uuid="eb1304d2-4773-4087-948a-21c7b11a63aa">
		<parameter name="cotizacion_tabla" class="java.lang.String"/>
		<queryString language="SQL">
			<![CDATA[SELECT
     tbl_cotizacion.`NUMERO_COTIZACION` AS tbl_cotizacion_NUMERO_COTIZACION,
     tbl_cotizacion.`FECHA_EMISION` AS tbl_cotizacion_FECHA_EMISION,
     tbl_cotizacion.`LUGAR_EMISION` AS tbl_cotizacion_LUGAR_EMISION,
     tbl_cotizacion.`VALIDEZ_OFERTA` AS tbl_cotizacion_VALIDEZ_OFERTA,
     tbl_cotizacion.`DESCUENTO` AS tbl_cotizacion_DESCUENTO,
     tbl_cotizacion.`IVA` AS tbl_cotizacion_IVA,
     tbl_cotizacion.`FECHA_CIERRE_EFECTIVO` AS tbl_cotizacion_FECHA_CIERRE_EFECTIVO,
     tbl_cotizacion.`CANCELADA` AS tbl_cotizacion_CANCELADA,
     tbl_cotizacion.`VISITA` AS tbl_cotizacion_VISITA,
     tbl_cotizacion.`PRESTAMO_MUESTRA` AS tbl_cotizacion_PRESTAMO_MUESTRA,
     tbl_cotizacion.`RELACION_MUESTRA` AS tbl_cotizacion_RELACION_MUESTRA,
     tbl_cotizacion.`NUMERO_REMISION` AS tbl_cotizacion_NUMERO_REMISION,
     tbl_cotizacion.`ESTADO` AS tbl_cotizacion_ESTADO,
     tbl_cotizacion.`FECHA_FACTURACION` AS tbl_cotizacion_FECHA_FACTURACION,
     tbl_cotizacion.`TBL_CLIENTE_ID_CLIENTE` AS tbl_cotizacion_TBL_CLIENTE_ID_CLIENTE,
     tbl_cotizacion.`TBL_PROPUESTA_NO_INCLUYE_ID_PROPUESTA_NO_INCLUYE` AS tbl_cotizacion_TBL_PROPUESTA_NO_INCLUYE_ID_PROPUESTA_NO_INCLUYE,
     tbl_cotizacion.`TBL_TIEMPO_ENTREGA_ID_TIEMPO_ENTREGA` AS tbl_cotizacion_TBL_TIEMPO_ENTREGA_ID_TIEMPO_ENTREGA,
     tbl_cotizacion.`TBL_DESCUENTO_VOLUMEN_ID_DESCUENTO_VOLUMEN` AS tbl_cotizacion_TBL_DESCUENTO_VOLUMEN_ID_DESCUENTO_VOLUMEN,
     tbl_cotizacion.`TBL_MODALIDAD_DE_PAGO_ID_MODALIDAD_DE_PAGO` AS tbl_cotizacion_TBL_MODALIDAD_DE_PAGO_ID_MODALIDAD_DE_PAGO,
     tbl_cotizacion.`TBL_LUGARES_ENTREGA_ID_LUGARES_ENTREGA` AS tbl_cotizacion_TBL_LUGARES_ENTREGA_ID_LUGARES_ENTREGA,
     tbl_cotizacion_producto.`ID_COTIZACION_PRODUCTO` AS tbl_cotizacion_producto_ID_COTIZACION_PRODUCTO,
     tbl_cotizacion_producto.`TBL_PRODUCTO_ID_PRODUCTO` AS tbl_cotizacion_producto_TBL_PRODUCTO_ID_PRODUCTO,
     tbl_cotizacion_producto.`TBL_COTIZACION_NUMERO_COTIZACION` AS tbl_cotizacion_producto_TBL_COTIZACION_NUMERO_COTIZACION,
     tbl_cotizacion_producto.`PRECIO_PARA_CLIENTE` AS tbl_cotizacion_producto_PRECIO_PARA_CLIENTE,
     tbl_cotizacion_producto.`PRECIO_BASE` AS tbl_cotizacion_producto_PRECIO_BASE,
     tbl_cotizacion_producto.`CANTIDAD` AS tbl_cotizacion_producto_CANTIDAD,
     tbl_producto.`ID_PRODUCTO` AS tbl_producto_ID_PRODUCTO,
     tbl_producto.`REFERENCIA` AS tbl_producto_REFERENCIA,
     tbl_producto.`DESCRIPCION` AS tbl_producto_DESCRIPCION,
     tbl_producto.`FECHA_ACTUALIZACION` AS tbl_producto_FECHA_ACTUALIZACION,
     tbl_producto.`PRECIO` AS tbl_producto_PRECIO,
     tbl_producto.`TBL_FABRICANTE_ID_FABRICANTE` AS tbl_producto_TBL_FABRICANTE_ID_FABRICANTE,
     tbl_producto.`TBL_SUBCATEGORIA_ID_SUBCATEGORIA` AS tbl_producto_TBL_SUBCATEGORIA_ID_SUBCATEGORIA,
     tbl_producto.`PERSONALIZADO` AS tbl_producto_PERSONALIZADO
FROM
     `tbl_cotizacion` tbl_cotizacion INNER JOIN `tbl_cotizacion_producto` tbl_cotizacion_producto ON tbl_cotizacion.`NUMERO_COTIZACION` = tbl_cotizacion_producto.`TBL_COTIZACION_NUMERO_COTIZACION`
     INNER JOIN `tbl_producto` tbl_producto ON tbl_cotizacion_producto.`TBL_PRODUCTO_ID_PRODUCTO` = tbl_producto.`ID_PRODUCTO`
	where TBL_COTIZACION_NUMERO_COTIZACION = $P{cotizacion_tabla}]]>
		</queryString>
		<field name="tbl_cotizacion_producto_TBL_COTIZACION_NUMERO_COTIZACION" class="java.lang.String"/>
		<field name="tbl_cotizacion_producto_PRECIO_PARA_CLIENTE" class="java.lang.Float"/>
		<field name="tbl_cotizacion_producto_CANTIDAD" class="java.lang.Integer"/>
		<field name="tbl_producto_REFERENCIA" class="java.lang.String"/>
		<field name="tbl_producto_DESCRIPCION" class="java.lang.String"/>
	</subDataset>
	<parameter name="numero_cotizacion" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select *, de.nombre as NOMBRE_DEPARTAMENTO, u.nombre as nombre_us, tie.descripcion as des_tiempo, dv.descripcion as des_descripcion, le.descripcion as lug_descripcion, pni.descripcion as propuesta_descripcion from tbl_cotizacion as c
inner join tbl_cotizacion_producto as cp
on c.numero_cotizacion = cp.tbl_cotizacion_numero_cotizacion
inner join tbl_cliente as cl
on c.tbl_cliente_id_cliente = cl.id_cliente
inner join tbl_direccion as d
on cl.id_cliente = d.tbl_cliente_id_cliente
inner join tbl_ciudad as ci
on d.tbl_ciudad_id_ciudad = ci.id_ciudad
inner join tbl_departamento as de
on ci.tbl_departamento_id_departamento = de.id_departamento
inner join tbl_email as e
on cl.id_cliente = e.tbl_cliente_id_cliente
inner join tbl_telefono as t
on cl.id_cliente = t.tbl_cliente_id_cliente
inner join tbl_usuario as u
on cl.tbl_usuario_id_usuario = u.id_usuario
inner join tbl_modalidad_de_pago as mp
on c.tbl_modalidad_de_pago_id_modalidad_de_pago = mp.id_modalidad_de_pago
inner join tbl_tiempo_entrega as te
on c.tbl_tiempo_entrega_id_tiempo_entrega = te.id_tiempo_entrega
inner join tbl_lugares_entrega as le
on c.tbl_lugares_entrega_id_lugares_entrega = le.id_lugares_entrega
inner join tbl_descuento_volumen as dv
on c.tbl_descuento_volumen_id_descuento_volumen = dv.id_descuento_volumen
inner join tbl_tiempo_entrega as tie
on c.tbl_tiempo_entrega_id_tiempo_entrega = tie.id_tiempo_entrega
inner join tbl_propuesta_no_incluye as pni
on c.tbl_propuesta_no_incluye_id_propuesta_no_incluye = pni.id_propuesta_no_incluye
where numero_cotizacion = $P{numero_cotizacion}
group by numero_cotizacion]]>
	</queryString>
